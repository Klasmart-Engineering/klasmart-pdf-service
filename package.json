{
  "name": "kidsloop-pdf-service",
  "version": "0.1.1",
  "description": "Service which stores and provides access to PDF content by converting pages into images and serving those images.",
  "repository": "https://bitbucket.org/calmisland/kidsloop-pdf-service",
  "main": "app.ts",
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "start:local": "ts-node -r dotenv/config ./src/app.ts",
    "start:dist": "node -r dotenv/config ./dist/app.js",
    "test": "npm run test:unit",
    "test:unit": "AWS_BUCKET=sample-bucket AWS_REGION=ap-northeast-2 NODE_ENV=test LOG_STYLE=SILENT nyc ts-mocha \"__tests__/unit/*.test.ts\"",
    "test:integration": "LOG_STYLE=SILENT nyc ts-mocha \"__tests__/integration/*.test.ts\"",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "cover:report": "nyc report --reporter=lcov --reporter=text",
    "build": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "lint": "eslint"
  },
  "author": {
    "name": "Mitch Goshorn",
    "email": "mitch.goshorn@calmid.com"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.18.0",
    "aws-sdk": "^2.923.0",
    "axios": "^0.21.1",
    "canvas": "^2.8.0",
    "core-js": "^3.14.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "node-cache": "^5.1.2",
    "pdfjs-dist": "^2.8.335",
    "pg": "^8.6.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.7",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.2.2",
    "@types/node-cache": "^4.2.5",
    "@types/pdfjs-dist": "^2.7.2",
    "@types/pug": "^2.0.4",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "minimatch": "^3.0.4",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "sinon": "^11.1.1",
    "stream-mock": "^2.0.5",
    "ts-mocha": "^8.0.0",
    "supertest": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
