{
  "name": "kidsloop-pdf-service",
  "version": "0.3.30-dev.1",
  "description": "Service which stores and provides access to PDF content by converting pages into images and serving those images.",
  "repository": "https://bitbucket.org/calmisland/kidsloop-pdf-service",
  "main": "app.ts",
  "scripts": {
    "release": "standard-version",
    "prerelease": "standard-version --prerelease dev",
    "start": "ts-node ./src/app.ts",
    "start:local": "node --max_old_space_size=8192 -r ts-node/register -r dotenv/config ./src/app.ts",
    "start:dist": "node -r dotenv/config ./dist/app.js",
    "test": "npm run test:unit",
    "test:unit": "LOG_LEVEL=silly LOG_STYLE=SILENT AWS_BUCKET=sample-bucket AWS_REGION=ap-northeast-2 NODE_ENV=test NEW_RELIC_ENABLED=false nyc ts-mocha \"__tests__/unit/**/*.test.ts\"",
    "test:integration": "LOG_LEVEL=silly LOG_STYLE=STRING_COLOR NEW_RELIC_ENABLED=false DEV_JWT_SECRET=pdf-integration-test-secret nyc ts-mocha -g \"GET /:prefix/validate/:key\" \"__tests__/integration/*.test.ts\"",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "cover:report": "nyc report --reporter=lcov --reporter=text",
    "build": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "lint": "eslint",
    "postinstall": "sh ./post-install.sh"
  },
  "author": {
    "name": "Mitch Goshorn",
    "email": "mitch.goshorn@calmid.com"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "3.10.0",
    "@newrelic/winston-enricher": "^2.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.2.2",
    "aws-sdk": "^2.923.0",
    "axios": "^0.21.4",
    "canvas": "^2.8.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.14.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "kidsloop-nodejs-logger": "bitbucket:calmisland/kidsloop-nodejs-logger#v0.2.12",
    "kidsloop-token-validation": "bitbucket:calmisland/kidsloop-token-validation#v1.2.2",
    "newrelic": "^8.5.0",
    "node-cache": "^5.1.2",
    "pg": "^8.6.0",
    "pug": "^3.0.2",
    "puppeteer": "^10.1.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typescript": "^4.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^8.2.2",
    "@types/node-cache": "^4.2.5",
    "@types/pug": "^2.0.4",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jsonwebtoken": "^8.5.1",
    "minimatch": "^3.0.4",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "sinon": "^11.1.1",
    "standard-version": "^9.3.2",
    "stream-mock": "^2.0.5",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0"
  }
}
